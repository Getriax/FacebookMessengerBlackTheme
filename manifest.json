{
  "manifest_version": 2,

  "name": "MessengerBlack",
  "description": "Changes the motive of the facebook messenger",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
    "matches": ["https://www.facebook.com/messages/*"],
    "css": ["messenger.css"]
    }
  ],

  "permissions": [
    "webNavigation",
    "tabs",
    "https://www.facebook.com/messages/*"
  ]
}
